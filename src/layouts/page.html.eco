---
layout: 'default'
---
<%- @partial('masthead') %>

<main class="content" role="main">
  <article class="post page">

    <header class="post-header">
      <h1 class="post-title"><%= @document.title %></h1>
    </header>

    <section class="post-content">
      <%- @content %>
    </section>

  </article>
</main>